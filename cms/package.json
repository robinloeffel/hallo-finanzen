{
	"name": "@hallo-finanzen/cms",
	"version": "0.0.1",
	"private": true,
	"license": "UNLICENSED",
	"type": "module",
	"scripts": {
		"build": "sanity build",
		"deploy": "sanity deploy",
		"dev": "sanity dev",
		"se": "sanity schema extract --enforce-required-fields",
		"se:w": "chokidar 'schemas/**/*.ts' -c 'npm run se' --initial",
		"start": "sanity start",
		"tg": "sanity typegen generate",
		"tg:w": "chokidar '../app/src/lib/sanity/queries.ts' -c 'npm run tg' --initial",
		"watch": "concurrently npm:*:w"
	},
	"dependencies": {
		"@sanity/icons": "^3.5.7",
		"@sanity/locale-de-de": "^1.1.16",
		"@sanity/ui": "^2.11.4",
		"@sanity/vision": "^3.71.2",
		"@types/react": "^18.3.5",
		"chokidar-cli": "^3.0.0",
		"concurrently": "^9.1.2",
		"eslint": "^9.19.0",
		"eslint-config-sweet": "^22.1.3",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-is": "^18.3.1",
		"robins-tsconfig": "^1.0.0",
		"sanity": "^3.71.2",
		"styled-components": "^6.1.14",
		"typescript": "^5.7.3"
	}
}
